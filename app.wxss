/**app.wxss**/

view {
  box-sizing: border-box;
}
.container {
  height: 100%;
  width: 100%;
  display: flex;
  /* 设置坐标轴，来设置项目排列方向。 */
  flex-direction: column;
  /* 设置项目在主轴方向上对齐方式，以及分配项目之间及其周围多余的空间。 */
  box-sizing: border-box;
  border-top: 5rpx solid #f0f3f8;
}


.dividing {
  width: 100%;
  height: 13rpx;
  background-color:#eeeeee;
}
/* button {
  flex-shrink: 0;
} */


.page {
  height: 100%;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
}
.navbar-title {
  color: red;
}
.view-page {
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  position: absolute;
  overflow: hidden;
}

.page-content {
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
}

.containers {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}